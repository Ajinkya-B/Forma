{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    /* ðŸ”‘ OVERRIDES */
    "module": "CommonJS",
    "moduleResolution": "node",

    /* Nest-required */
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "useDefineForClassFields": false,

    /* General */
    "strict": true,
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
